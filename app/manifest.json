{
  "author": "Elliott Linder",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["inject.js"],
    "run_at": "document_end"
  }],
  "default_locale": "en",
  "description": "Quickly submit forms using Ctrl+Shift hotkey",
  "homepage_url": "http://linder.bz",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "name": "Form hotkey submit",
  "short_name": "FHS",
  "version": "0.0.1",
  "web_accessible_resources": ["dist/main.js"]
}
